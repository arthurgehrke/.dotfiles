Host *
  Compression yes
  AddKeysToAgent yes
  ForwardX11 yes
  IdentitiesOnly yes
  IdentityFile ~/.ssh/github
  ServerAliveInterval 240
  ServerAliveCountMax 2
  TCPKeepAlive yes

Host github.com
  Hostname ssh.github.com
  Port 443
  User git

Host gitlab.com
  IdentityFile ~/.ssh/gitlab
  ServerAliveCountMax 5

